
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
    <xs:element name="validarcontidad">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="cantidad">
                    <xs:simpleType>
                        <xs:restriction base="xs:integer">
                            <xs:minInclusive value="-30"/>
                            <xs:maxInclusive value="30"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:element>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
</xs:schema>